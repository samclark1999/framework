{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": null
    },
    "border": {
      "radius": true,
      "width": true
    },
    "custom": {
      "header-height": "96px"
    },
    "layout": {
      "contentSize": "100%"
    },
    "color": {
      "custom": true,
      "customDuotone": false,
      "customGradient": true,
      "link": true,
      "text": true,
      "background": true,
      "defaultGradients": false,
      "defaultPalette": true,
      "duotone": [],
      "gradients": [],
      "palette": []
    },
    "typography": {
      "fluid": true,
      "customFontSize": true,
      "lineHeight": true,
      "dropCap": false,
      "fontStyle": true,
      "fontWeight": false,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true,
      "fontSizes": [
        {
          "size": "14px",
          "slug": "body-small",
          "name": "Body Small"
        },
        {
          "size": "16px",
          "slug": "body-regular",
          "name": "Body Regular (default)"
        },
        {
          "size": "18px",
          "slug": "body-large",
          "name": "Body Large"
        }
      ]
    },
    "appearanceTools": true,
    "blocks": {
      "core/paragraph": {
        "spacing": {
          "margin": true
        }
      },
      "core/button": {
        "border": {
          "radius": false,
          "width": false
        }
      }
    }
  },
  "styles": {
    "color": {
    },
    "typography": {
    },
    "elements": {
      "link": {
        "color": {},
        "typography": {}
      }
    }
  }
}